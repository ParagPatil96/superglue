{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", ".next/**"],
        "inputs": ["src/**/*.tsx", "src/**/*.ts", "*.ts", "*.tsx", "package.json"]
      },
      "dev": {
        "dependsOn": ["build"],
        "cache": false,
        "persistent": true
      },
      "start": {
        "cache": false,
        "persistent": true
      },
      "test": {
        "dependsOn": ["@superglue/core#build"],
        "outputs": []
      },
      "test:coverage": {
        "dependsOn": ["@superglue/core#build"],
        "outputs": ["coverage/**"]
      }
    },
    "globalEnv": [
      "GRAPHQL_ENDPOINT",
      "GRAPHQL_PORT",
      "WEB_PORT",
      "DATASTORE_TYPE",
      "REDIS_HOST",
      "REDIS_PORT",
      "REDIS_USERNAME",
      "REDIS_PASSWORD",
      "NODE_ENV",
      "AUTH_TOKEN",
      "DEBUG",
      "MODEL_PROVIDER",
      "FIREWORKS_API_KEY",
      "FIREWORKS_MODEL",
      "FIREWORKS_BASE_URL",
      "NEXT_PUBLIC_SUPERGLUE_ENDPOINT",
      "NEXT_PUBLIC_SUPERGLUE_API_KEY",
      "LLM_PROVIDER",
      "LLM_MODEL",
      "SCHEMA_GENERATION_PROVIDER",
      "SCHEMA_GENERATION_MODEL",
      "GOOGLE_GENERATIVE_AI_API_KEY",
      "OPENAI_API_KEY",
      "ANTHROPIC_API_KEY",
      "MISTRAL_API_KEY",
      "DEEPSEEK_API_KEY",
      "XAI_API_KEY"
    ]
}
